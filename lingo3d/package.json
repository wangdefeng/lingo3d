{
	"name": "lingo3d",
	"version": "1.11.3",
	"description": "Lingo3D is a web-first 3d game development library with React integration",
	"homepage": "https://github.com/lingo3d/lingo3d",
	"author": "Lingo3D",
	"license": "MIT",
	"keywords": [
		"lingo3d",
		"3d",
		"game",
		"react"
	],
	"scripts": {
