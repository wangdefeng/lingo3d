		"@lincode/math": "*",
		"@lincode/promiselikes": "*",
		"@lincode/reactivity": "*",
		"@lincode/utils": "*",
		"@tweakpane/core": "^1.1.0",
		"@types/three": "0.141.0",
		"browser-fs-access": "0.29.6",
		"cannon-es": "0.19.0",
		"nipplejs": "^0.9.1",
		"preact": "^10.8.2",
		"preact-custom-element": "^4.2.1",
		"prettier": "^2.7.1",
		"three": "0.142.0",
		"tweakpane": "^3.1.0",
		"xstate": "*"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@preact/preset-vite": "^2.3.0",
